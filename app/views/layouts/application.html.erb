<!DOCTYPE html>
<html>
  <head>
    <title>Orsupport</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="sidebar">
      <div id="logo">
        <%= link_to root_path do %>
          <%= image_tag "logo.png" %>
        <% end %>
        <ul>
          <li class="category">Home</li>
          <li>HelpfulLinks</li>
        </ul>
      </div>
    </div>
    <%= yield %>
  </body>
</html>
